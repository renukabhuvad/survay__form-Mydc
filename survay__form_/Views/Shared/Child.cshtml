    <script src="~/FamilyFinancialSecurity/js/bootstrap.js"></script>
    <script src="~/FamilyFinancialSecurity/js/bootstrap.min.js"></script>
    <script src="~/FamilyFinancialSecurity/js/classie.js"></script>
    <script src="~/js/Validataions.js"></script>
    <script>
        $(document).ready(function () {
            $(".dropdown").hover(
                function () {
                    $('.dropdown-menu', this).stop(true, true).slideDown("fast");
                    $(this).toggleClass('open');
                },
                function () {
                    $('.dropdown-menu', this).stop(true, true).slideUp("fast");
                    $(this).toggleClass('open');
                }
            );
        });
    </script>

    @RenderSection("scripts", required: false)

</body>
</html>

    <style>
        @@media (max-width:767px) {
            .hidden-xs {
                display: none !important;
            }
        }

        @@media (min-width:768px) and (max-width:991px) {
            .hidden-sm {
                display: none !important;
            }
        }

        @@media (max-width:767px) {
            #return-to-top {
                display: none !important;
            }
        }

        @@media (min-width:768px) and (max-width:991px) {
            #return-to-top {
                display: none !important;
            }
        }
    </style>
    <style>
        input, textarea {
            margin-bottom: 0px !important;
            line-height: initial;
        }

        .back-section {
            position: relative;
            /*padding-bottom: 92px;*/
            /*background-image: url('../images/kazimrazabackgroundimage.jpg');*/
            /*width: 100%;
            height: 150px;
            background-size: cover;
            background-position: center center;
            background-repeat: no-repeat;
            background-attachment: fixed;*/
        }

            .back-section:before {
                position: absolute;
                content: '';
                /*background-color: #6a5cbc;*/
                background-image: linear-gradient(270deg, #d04d85, #534ab3);
                width: 100%;
                height: 100%;
                left: 0px;
                top: 0px;
            }

        .curved {
            position: relative;
            background: #2c3e50;
            height: 50vh;
            border-bottom-left-radius: 50% 20%;
            border-bottom-right-radius: 50% 20%;
        }

    </style>
    <style>
        @@media screen and (max-width: 520px) and (min-width: 220px) {
            h1 {
                padding-top: 10px;
                font-size: 35px;
                line-height: 30px;
                font-weight: 400;
                color: #fff;
                text-transform: uppercase;
            }

            .navbar-brand {
                padding: 0px !important;
            }

            .navbar-brand img 
            {
                width: 250px !important;
                padding-top: 5px;
            }

            .main-menu .navbar-header 
            {
                padding: 0px;
            }

            .back-section {
                padding-bottom: 0px;
                height: 94px;
            }

            .back-section h1 
            {
                padding-top: 20px !important;
                line-height: initial;
            }
        }

        a 
        {
            text-decoration: none;
        }

        th {
            background: #7e66f8 !important;
            border:1px solid #fff !important;
            color: #fff;
        }

        .navbar-toggle 
        {
            float: right !important;
            padding: 22px 19px !important;
            margin-right: 0px !important;
        }

        .back-section h1 
        {
            padding-top: 40px;
            font-size: 30px;
            font-weight: 300;
        }
    </style>

    <link href="~/css/style.css" rel="stylesheet" />
    <link href="~/css/responsive.css" rel="stylesheet" />


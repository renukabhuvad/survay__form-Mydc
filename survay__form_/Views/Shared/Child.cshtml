    <link href="~/css/custom/jssocials-theme-classic.css" rel="stylesheet" />
    <script src="~/css/custom/jssocials.js"></script>
    <script src="~/css/custom/jssocials.min.js"></script>

    <script type="text/javascript">
        $(window).ready(function ()
        {
            $("#shareIcons").jsSocials({
                showLabel: false,
                showCount: false,
                shares: ["facebook", "linkedin", "twitter", "googleplus", "whatsapp", "instagram"]
            });
        });

    </script>

    <link href="~/css/custom/datepicker.min.css" rel="stylesheet" />


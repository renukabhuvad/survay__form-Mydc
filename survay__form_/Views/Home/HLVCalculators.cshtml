function fnValidate1()
{
	//debugger;
	strName = document.getElementById("txtName").value;
	strDOB =  document.getElementById("txtDob").value;
	intAge = document.getElementById("txtAge").value;
    intRetmtAge = document.getElementById("txtRetmtAge").value;
    intInflationRate = document.getElementById("txtInflationRate").value;

		if(strName == "")
        {
		    fnAlert('txtName','Enter your name');
		    document.getElementById("txtName").style.borderColor = "#ff0000";
		    return false;
		}
		else if(strDOB == "")
		{
		  fnAlert('txtDob','Select DOB');
		  document.getElementById("txtDob").style.borderColor = "#ff0000";
		  return false;
		}
		else if(parseInt(intAge) < 0)
		{
		  fnAlert('txtDob','Select proper DOB');
		  document.getElementById("txtDob").style.borderColor = "#ff0000";
		  return false;
		}
		else if(intRetmtAge == "")
		{
		  fnAlert('txtRetmtAge','Enter your retirement age');
		  document.getElementById("txtRetmtAge").style.borderColor = "#ff0000";
		  return false;
		}
		else if(parseInt(intAge) >= parseInt(intRetmtAge))
		{
		  fnAlert('txtRetmtAge','Retirement age should be greater than date of birth');
		  document.getElementById("txtRetmtAge").style.borderColor = "#ff0000";
		  return false;
        }
        if (intInflationRate == "" || parseInt(intInflationRate) < 0) {
            fnAlert('txtInflationRate', 'Enter inflation rate');
            document.getElementById("txtInflationRate").style.borderColor = "#ff0000";
            return false;
        }
		else if(strGender == "")
		{
		  fnAlert('','Select your gender');
		  return false;
		}
		else
		{
			$("#myCarousel").carousel(1);
		}

}



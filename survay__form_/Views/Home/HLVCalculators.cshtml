function fnValidate9()
{
		//debugger;

		if(strInsured == "Not Insured")
		{
			intCurrLifeCover = 0;
		}
		else
		{
			intCurrLifeCover =  document.getElementById("txtCurrLifeCover").value;
			if(intCurrLifeCover == "")
			{
			fnAlert('txtCurrLifeCover','Enter your current life cover');
			document.getElementById("txtCurrLifeCover").style.borderColor = "#ff0000";
			return false;
			}
		}


			document.getElementById("txtMyIncome").value = intAnnIncome;
            document.getElementById("txtSpouseIncome").value = intSpouseAnnSalary;
            document.getElementById("txtSpouseIncomeGrowthPercentage").value = intSpouseIncomeGrowthPercentage;
			document.getElementById("txtExpenditure").value = intHouseExpnse ;
			document.getElementById("txtCurLiab").value = intCurrLiability;
			document.getElementById("txtLifeCover").value = intCurrLifeCover;

			intAnnualIncome =  parseInt(intAnnIncome) + parseInt(intSpouseAnnSalary);

			jQuery.support.cors = true;

			var HLVCalcInput =
			{
			  Name: strName,
			  DOB: strDOB,
			  Age: intAge,
			  RetirementAge: intRetmtAge,
              AnnualIncome: intAnnualIncome,
              SpouseAnnualIncome: intSpouseAnnSalary,
              SpouseIncomeGrowthPercentage: intSpouseIncomeGrowthPercentage,
			  AnnualExpenses: intHouseExpnse,
              InflationRate: intInflationRate,
              LifeInsuranceCover: intCurrLifeCover,
              CurrentLiabilities: intCurrLiability,
              LiquidableSavings: intCurrLiquidAssets
			};
		 $.ajax({

            url: 'hlvCalculator',
			type: 'POST',
			data: JSON.stringify(HLVCalcInput),
			contentType: "application/json;charset=utf-8",
			dataType: "text",
            success: function (data) {
				var json = $.parseJSON(data);

                hlvValue = json.HumanLifeValue;
                hlvmessage = json.HLVMessage;


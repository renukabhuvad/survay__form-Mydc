        else if (intRetmtAge == "") {
            fnAlert('txtRetmtAge', 'Enter your retirement age');
            document.getElementById("txtRetmtAge").style.borderColor = "#ff0000";
            return false;
        }
        else if (atpos < 1 || dotpos < atpos + 2 || dotpos + 2 >= strEmail.length) {
            fnAlert('txtemail', 'Enter your correct Email');
            document.getElementById("txtemail").style.borderColor = "#ff0000";
            return false;
        }
        else if (intMobile == "" || isNaN(intMobile) || intMobile.length != 10) {
            fnAlert('txtmobile', 'Enter Correct Mobile No');
            document.getElementById("txtmobile").style.borderColor = "#ff0000";
            return false;
        }
        else if (parseInt(intAge) >= parseInt(intRetmtAge)) {
            fnAlert('txtRetmtAge', 'Retirement age should be greater than date of birth');
            document.getElementById("txtRetmtAge").style.borderColor = "#ff0000";
            return false;
        }

        if (intInflationRate == "" || parseInt(intInflationRate) < 0) {
            fnAlert('txtInflationRate', 'Enter inflation rate');
            document.getElementById("txtInflationRate").style.borderColor = "#ff0000";
            return false;
        }
        else if (strGender == "") {
            fnAlert('', 'Select your gender');
            return false;
        }

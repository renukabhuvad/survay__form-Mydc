    function fnAgeCalc() {
        //debugger;
        var today = new Date();
        var stDOB = document.getElementById("txtDob").value;
        var datearray = stDOB.split('/');
        var newdate = datearray[1] + '/' + datearray[0] + '/' + datearray[2];
        var birthDate = new Date(newdate);
        intAGE = today.getFullYear() - birthDate.getFullYear();
        var m = today.getMonth() - birthDate.getMonth();
        if (m < 0 || (m === 0 && today.getDate() < birthDate.getDate())) {
            intAGE--;
        }


        if (today < strDobAge) {
            fnAlert('txtDob', 'DOB should be less than current date');
            document.getElementById("txtDob").style.borderColor = "#ff0000";
            document.getElementById("txtAge").value = "";
            return false;


        }
        else {
            document.getElementById("txtAge").value = intAGE;
        }

    }


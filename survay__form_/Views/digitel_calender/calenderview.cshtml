@model survay__form_.Models.policyentryview
@{
    Layout = null;
}

<html>
<head>
    <title> Digital Calender Agent </title>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="keywords" content="">
    <meta name="description" content="Premium Calender">

    <!-- Schema.org markup for Google+ -->
    <meta itemprop="name" content="Digital Calender View">
    <meta itemprop="description" content="">

    <!-- Twitter Card data -->
    <meta name="twitter:site" content="">
    <meta name="twitter:title" content="Digital Calender View">
    <meta name="twitter:description" content="">
    <meta name="twitter:creator" content="">
    <meta name="twitter:image:src" content="http://mydc.in/survay/images/favicon.png">

    <!-- Open Graph data -->
    <meta property="og:title" content="Digital Calender View" />
    <meta property="og:type" content="Digital Calender View" />
    <meta property="og:description" content="" />
    <meta property="og:site_name" content="" />
    <meta property="og:image" content="http://mydc.in/survay/images/favicon.png" />
    <meta property="article:section" content="Premium Calender" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.css" rel="stylesheet" type="text/css" />
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>

    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link href="https://fonts.googleapis.com/css2?family=Asap:wght@400;500;600;700&display=swap" rel="stylesheet" />
    <link rel="icon" href="http://mydc.in/survay/images/favicon.png" type="image/x-icon">
    <style>
        .borderbottom {
            display: block;
            border-bottom: 1px dashed #dcdcdc;
            padding-bottom: 5px;
        }
    </style>
</head>
<body>

    <style>
        body {
            font-family: "Asap", sans-serif;
        }

        .col-md-6 {
            width: 50%;
            float: left;
        }

        .action {
            float: right;
            margin: 8px 5px;
            background: #0f3987;
            padding: 0px 5px;
            font-size: 13px;
            color: #fff;
            border-radius: 5px;
        }

        h6 {
            padding-top: 10px;
            padding-left: 10px;
        }

        .page-main {
            border: #66aa05 2px solid;
            margin: 10px;
            border-radius: 10px;
        }

        a, a:hover {
            text-decoration: none;
        }

        @@media only screen and (max-width:520px) and (min-width:220px) {
            .col-xs-4 {
                width: 33.33333333%;
            }

            .col-xs-5 {
                width: 41.66666667%;
            }

            .col-xs-7 {
                width: 58.33333333%;
            }
        }
    </style>

    <div style="background:#66aa05;">
        <span style="color: #fff;text-align: center;margin: auto;display: block;padding: 12px 0px;font-size: 23px;"> Digital Calender View </span>
    </div>
    <div>
        <div class="content">

            <div class="box" style="margin:10px;">
                <div>
                    <input type="text" class="form-control" id="search-criteria" placeholder="Search" />
                </div>
            </div>

            <div>
                @{
                    foreach (var item in Model.policyentries)
                    {
                        <div class="tpasearch">
                            <div class="page-main">
                                <div class="col-md-12">
                                    <div class="row">
                                        <div class="col-md-6 col-sm-5 col-xs-5" style="padding:0px;">
                                            <h6> <b> @item.fullname </b> </h6>
                                        </div>
                                        <div class="col-md-6 col-sm-7 col-xs-7" style="padding:0px;">
                                            <a href="#" class="action" onclick="policyedit(@item.policyentryid);" style="background: #d3fbf1;color: #47c5a7;border: 1px solid;"><i class="fa fa-edit"></i> Edit </a>
                                            <a href="#" class="action" onclick="policyview(@item.policyentryid);" style="background: #e0ebff;color: #0f3987;border: 1px solid;"><i class="fa fa-bars"></i> View </a> @*data-toggle="modal" data-target="#ViewModal"*@
                                            <a href="#" class="action" onclick=" showshare('@item.familymemberid', @ViewBag.auth_key,@item.mobileno);" id="share" style="background: #d5f2ff;color:#36afe3;border: 1px solid;"><i class="fa fa-share"> </i> Share </a> @*../digitel_calender/member?id=@item.policyentryid&auth_key=@ViewBag.auth_key*@
                                        </div>
                                    </div>
                                    <hr style="margin-top:5px;margin-bottom:5px;">
                                </div>
                                <div class="caption">
                                    <div class="piko-title6 two" style="margin-bottom: 20px;margin-left: 10px;">
                                        <label> Policy No: @item.policyno </label>
                                        <label> Product Name : @item.Insurance </label>
                                        <label> Sum Assured : @item.Insurer </label>
                                        <label> Policy Next Due : @item.fupdate </label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    }
                }
            </div>
        </div>
    </div>
    @*</div>
        </div>*@

    <style>
        label {
            display: block;
        }
    </style>

    <!-- View Modal -->
    <div class="modal fade" id="ViewModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel"> </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body" id="viewmodel1">

                </div>
            </div>
        </div>
    </div>
    <!-- End View Modal -->
    <!-- Edit Modal -->
    <div class="modal fade" id="EditModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel1">abxcd </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form method="post" action="savedata">
                        <input type="hidden" value="0" id="hdnpolicyid" name="policyentryid" />
                        <div class="form-group">
                            <label> Policy No : </label>
                            <input type="text" class="form-control" id="txtpolicyno" name="policyno" onKeyPress="if(this.value.length==50) return false;">
                        </div>

                        <div class="form-group">
                            <label> Mobile No :  </label>
                            <input type="number" class="form-control" id="txtmobileno" name="mobile" onKeyPress="if(this.value.length==10) return false;">
                        </div>
                        <div class="form-group">
                            <label> Date Of Birth:  </label>
                            <div class="form-group">
                                <div class='input-group date form-floating' id='datetimepicker1'>
                                    <input type='text' class="form-control" id="floatingInput" name="dob" readonly />
                                    <span class="input-group-addon input-group-text">
                                        <span class="fa fa-calendar"></span>
                                    </span>
                                </div>
                            </div>
                            <div class="form-group">
                                <label> Product Name : </label>
                                <input type="text" class="form-control" id="txtproduct" name="productname">
                            </div>

                            <div class="form-group">
                                <label> Sum Assured :  </label>
                                <input type="number" class="form-control" id="txtsumassured" name="sumass" onKeyPress="if(this.value.length==14) return false;">
                            </div>
                            <div class="form-group">
                                <label> Mode :  </label>
                                <select class="form-control" id="ddlmode" name="mode" onchange="date_change();">
                                    <option value="0">---Select---</option>
                                    <option value="Y">Yearly</option>
                                    <option value="H">Half Yearly</option>
                                    <option value="Q">Quaterly</option>
                                    <option value="M">Monthly</option>
                                    <option value="S">Single</option>
                                </select>
                            </div>
                            <div class="form-group">
                                <label> Term :  </label>
                                <input type="number" class="form-control" id="txtterm" name="term" onKeyPress="if(this.value.length==2) return false;">
                            </div>
                            <div class="form-group">
                                <label> PPT :  </label>
                                <input type="number" class="form-control" id="txtppt" name="ppt" onKeyPress="if(this.value.length==2) return false;">
                            </div>
                            <div class="form-group">
                                <label> Policy Start Due :  </label>
                                @*<input type="text" class="form-control" id="txtpsd" name="psd">*@
                                <div class="form-group">
                                    <div class='input-group date form-floating' id='datetimepicker2'>
                                        <input type='text' class="form-control" id="txtpsd" name="psd" readonly onchange="date_change();" />
                                        <span class="input-group-addon input-group-text">
                                            <span class="fa fa-calendar"></span>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <label> Policy Next Due :  </label>
                                @*<input type="text" class="form-control" id="txtpnd" name="nxtdue">*@
                                <div class="form-group">
                                    <div class='input-group date form-floating' id='datetimepicker3'>
                                        <input type='text' class="form-control" id="txtpnd" name="nxtdue" readonly />
                                        <span class="input-group-addon input-group-text">
                                            <span class="fa fa-calendar"></span>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <label> Premium :  </label>
                                <input type="number" class="form-control" id="txtpremium" name="premium" onKeyPress="if(this.value.length==9) return false;">
                            </div>
                            <input type="submit" class="btn btn-primary" value="Update" />


                        </div>
                    </form>
                </div>
            </div>
        </div>
        <!-- End Edit Modal -->
    </div>
    <script>
        $("#search-criteria").on("keyup", function () {
            var g = $(this).val().toLowerCase();
            $(".tpasearch").each(function () {
                var s = $(this).text().toLowerCase();
                $(this).closest('.tpasearch')[s.indexOf(g) !== -1 ? 'show' : 'hide']();
            });
        });
    </script>

    <!-- Share Modal -->
    <div class="modal fade" id="ShareModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel"> Share </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="col-md-4 col-sm-4 col-xs-4" style="float: left;">
                                <a role="button" onclick="shareEmail();"> <img src="~/images/gmail (1).png" class="img-responsive" style="width: 64px;" /></a>
                            </div>
                            <div class="col-md-4 col-sm-4 col-xs-4" style="float: left;">
                                <a role="button" onclick="shareWhatsapp();"><img src="~/images/whatsapp (3).png" class="img-responsive" /></a>
                            </div>
                            <div class="col-md-4 col-sm-4 col-xs-4" style="float: left;">
                                <a role="button" onclick="shareSMS();"><img src="~/images/sms.png" class="img-responsive" /></a>
                            </div>
                            <div class='' id='slink'>
                                <div class='row' style="margin:0px;padding:10px;">

                                    <input type='text' id="txturl" class='form-control' style='margin-bottom: 10px;' readonly />

                                </div>
                            </div>
                            <div class='' id='semail'>
                                <div class='row' style="margin:0px;padding:10px;">
                                    <label> Enter Email Id </label>
                                    <input type='text' id="txtemail" class='form-control' style='margin-bottom: 10px;' />
                                    <a href='#' class='btn btn-primary' onclick="sendmail();"> Send </a>
                                </div>
                            </div>
                            <div class='' id='swhatsapp'>
                                <div class='row' style="margin:0px;padding:10px;">
                                    <label> Enter Whatsapp No </label>
                                    <input type='number' id="txtwano" class='form-control' style='margin-bottom: 10px;' onKeyPress="if(this.value.length==10)" />
                                    <a href='#' class='btn btn-primary' onclick="sendwhatsapp();"> Send </a>
                                </div>
                            </div>
                            <div class='' id='ssms'>
                                <div class='row' style="margin:0px;padding:10px;">
                                    <label> Enter Number </label>
                                    <input type='number' id="txtsmsno" class='form-control' style='margin-bottom: 10px;' onKeyPress="if(this.value.length==10)" />
                                    <a href='#' class='btn btn-primary' onclick="sendsms();"> Send </a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- End Share Modal -->


    <link href="/survay/DateTimePicker/bootstrap-datetimepicker.css" rel="stylesheet">
    <script src="/survay/DateTimePicker/bootstrap-datepicker.min.js"></script>
    <link href="/survay/DateTimePicker/bootstrap-datetimepicker.min.css" rel="stylesheet" />
    <script src="/survay/DateTimePicker/bootstrap-datetimepicker.js"></script>
    <link href="/survay/DateTimePicker/datepicker.min.css" rel="stylesheet" />
    <script src="/survay/DateTimePicker/bootstrap-datetimepicker.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.18.1/moment.min.js"></script>
    <script>

        document.getElementById("semail").style.display = "none";
        document.getElementById("swhatsapp").style.display = "none";
        document.getElementById("ssms").style.display = "none";
        document.getElementById("slink").style.display = "none";

        function shareEmail() {
            document.getElementById("slink").style.display = "block";
            document.getElementById("semail").style.display = "block";
            document.getElementById("swhatsapp").style.display = "none";
            document.getElementById("ssms").style.display = "none";
        }

        function shareWhatsapp() {
            document.getElementById("slink").style.display = "block";
            document.getElementById("swhatsapp").style.display = "block";
            document.getElementById("semail").style.display = "none";
            document.getElementById("ssms").style.display = "none";
        }

        function shareSMS() {
            document.getElementById("slink").style.display = "block";
            document.getElementById("ssms").style.display = "block";
            document.getElementById("semail").style.display = "none";
            document.getElementById("swhatsapp").style.display = "none";
        }
    </script>
    <script>
            $(document).ready(function () {

                $('#datetimepicker1').datepicker({
                    format: 'dd/mm/yyyy',
                    autoclose: true
                });
                $('#datetimepicker2').datepicker({
                    format: 'dd/mm/yyyy',
                    autoclose: true
                });
                $('#datetimepicker3').datepicker({
                    format: 'dd/mm/yyyy',
                    autoclose: true
                });


                $("#txtmobileno").keypress(function (e) {
                    //if the letter is not digit then display error and don't type anything
                    if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
                        //display error message
                        // $("#errmsg").html("Digits Only").show().fadeOut("slow");
                        return false;
                    }
                });
                $("#txtpolicyno").keypress(function (e) {
                    //if the letter is not digit then display error and don't type anything
                    if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
                        //display error message
                        // $("#errmsg").html("Digits Only").show().fadeOut("slow");
                        return false;
                    }
                });
                $("#txtsumassured").keypress(function (e) {
                    //if the letter is not digit then display error and don't type anything
                    if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
                        //display error message
                        // $("#errmsg").html("Digits Only").show().fadeOut("slow");
                        return false;
                    }
                });
                $("#txtterm").keypress(function (e) {
                    //if the letter is not digit then display error and don't type anything
                    if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
                        //display error message
                        // $("#errmsg").html("Digits Only").show().fadeOut("slow");
                        return false;
                    }
                });
                $("#txtppt").keypress(function (e) {
                    //if the letter is not digit then display error and don't type anything
                    if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
                        //display error message
                        // $("#errmsg").html("Digits Only").show().fadeOut("slow");
                        return false;
                    }
                });
                $("#txtpremium").keypress(function (e) {
                    //if the letter is not digit then display error and don't type anything
                    if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
                        //display error message
                        // $("#errmsg").html("Digits Only").show().fadeOut("slow");
                        return false;
                    }
                });

            });

            function policyview(id) {
                $.getJSON('@Url.Action("policyview", "digitel_calender")' + "?id=" + id + "&auth_key=" + @ViewBag.auth_key, function (result) {

                    if (result.data.policyentryid != "0") {
                        $("#exampleModalLabel").html(result.data[0].fullname);
                        $("#viewmodel1").empty();
                        var html = "";
                        html = "<label class='borderbottom'> Policy No: " + result.data[0].policyno+" </label>";
                        html += "<label class='borderbottom'> Mobile No : " + result.data[0].mobileno +" </label>";
                        html += "<label class='borderbottom'> Date Of Birth: " + result.data[0].dateofbirth +" </label>";
                        html += "<label class='borderbottom'> Product Name : " + result.data[0].Insurance +" </label>";

                        html += "<label class='borderbottom'> Sum Assured :" + result.data[0].sumassured +" </label>";
                        html += "<label class='borderbottom'> Mode : " + result.data[0].mode +" </label>";
                        html += "<label class='borderbottom'> Term / PPT : " + result.data[0].term + "/ " + result.data[0].ppt +" </label>";
                        html += "<label class='borderbottom'> Policy Start Due : " + result.data[0].docstartdate +" </label>";
                        html += "<label class='borderbottom'>Policy Next Due :" + result.data[0].fupdate +" </label>";
                        html += "<label class='borderbottom'> Premium : " + result.data[0].premium +" </label>";
                        $("#viewmodel1").append(html);
                        $("#ViewModal").modal('show');
                    }

        })
            }
            function policyedit(id) {
                $.getJSON('@Url.Action("policyview", "digitel_calender")' + "?id=" + id + "&auth_key=" + @ViewBag.auth_key, function (result) {

                    if (result.data.policyentryid != "0") {
                        $("#hdnpolicyid").val(result.data[0].policyentryid);
                        $("#exampleModalLabel1").html(result.data[0].fullname);
                        $("#txtpolicyno").val(result.data[0].policyno);
                        $("#txtmobileno").val(result.data[0].mobileno);
                        $("#floatingInput").val(result.data[0].dateofbirth);
                        $("#txtproduct").val(result.data[0].Insurance);

                        $("#txtsumassured").val(result.data[0].sumassured);
                        $("#ddlmode").val(result.data[0].mode);
                        $("#txtterm").val(result.data[0].term);
                        $("#txtppt").val(result.data[0].ppt);
                        $("#txtpsd").val(result.data[0].docstartdate);
                        $("#txtpnd").val(result.data[0].fupdate);
                        $("#txtpremium").val(result.data[0].premium);
                        $("#EditModal").modal('show');
                    }

        })
        }

        function showshare(id, authkey, mobileno)
        {
            var authkey = "@ViewBag.auth_key";
            $("#txturl").val("http://mydc.in/survay/digitel_calender/memberlist?id=" + id + "&auth_key=" + authkey + "");
            var m = "91" + mobileno;
            //alert(m);
            $("#txtsmsno").val(m);
            $("#txtwano").val(m);
            $("#ShareModal").modal();
        }

        function sendmail() {
            var encoded = encodeURIComponent($("#txturl").val());
            window.location.href = "mailto:" + $("#txtemail").val() + "?subject=Your Policy Details&body=" + encoded + "";
        }
        function sendwhatsapp() {
           whatsappopen();
        }
        function sendsms() {
            var encoded = encodeURIComponent($("#txturl").val());
            window.location.href = "sms:" + $("#txtsmsno").val() + "?body=" + encoded +"";
        }
		var isMobile = mobilecheck();
